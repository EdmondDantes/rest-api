{
    "name": "ifcastle/rest-api",
    "description": "Rest api layer.",
    "type": "ifcastle-api",
    "license": "MIT",
    "keywords": [
        "ifcastle",
        "rest",
        "api"
    ],
    "extra": {
        "ifcastle-installer": {
            "package": {
                "name": "rest-api",
                "bootloaders": [
                    "IfCastle\\RestApi\\Bootloader"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "IfCastle\\RestApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IfCastle\\RestApi\\": "./tests"
        }
    },
    "require": {
        "ifcastle/application": "^0.6.0",
        "ifcastle/protocol-contracts": "^0.5.0",
        "symfony/routing": "^7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.60",
        "amphp/php-cs-fixer-config": "^2.1",
        "phpunit/phpunit": "^11.3"
    }
}
