{
    "name": "ifcastle/rest-api",
    "description": "Rest api layer.",
    "type": "ifcastle-api",
    "license": "MIT",
    "keywords": [
        "ifcastle",
        "rest",
        "api"
    ],
    "extra": {
        "ifcastle-installer": {
            "package": {
                "name": "rest_api",
                "bootloaders": [
                    "IfCastle\\RestApi\\Bootloader"
                ],
                "tags": [
                    "rest_api"
                ]
            }
        }
    },
    "autoload": {
        "psr-4": {
            "IfCastle\\RestApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IfCastle\\RestApi\\": "./tests"
        }
    },
    "require": {
        "php": "^8.4",
        "ifcastle/application": "^0.8",
        "ifcastle/protocol-contracts": "^0.7",
        "symfony/routing": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "ifcastle/codestyle": "^0.1",
        "phpstan/phpstan": "^1.12"
    }
}
